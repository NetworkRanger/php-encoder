{
    "name": "iphpjs/php-encoder",
    "description": "The Iphpjs Encoding package.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "NetworkRanger",
            "email": "admin@iphpjs.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "iphpjs/support": "dev-master",
        "iphpjs/contracts": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Iphpjs\\Encoder\\": "src/"
